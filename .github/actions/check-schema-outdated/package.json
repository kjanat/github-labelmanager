{
  "name": "check-schema-outdated",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "actions"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "module": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "all": "bun run format:write && bun run lint && bun run test && bun run coverage && bun run package",
    "build": "bun build --target node --format esm --outdir dist --entry-naming=[name].[ext] src/main.ts",
    "bundle": "bun run format:write && bun run package",
    "ci-test": "bun test --coverage --coverage-reporter=lcov --reporter=junit --reporter-outfile=./junit.xml",
    "coverage": "bun test --coverage",
    "format:check": "bunx prettier --check .",
    "format:write": "bunx prettier --write .",
    "lint": "bunx eslint .",
    "local-action": "bunx @github/local-action . src/main.ts .env",
    "package": "tsdown",
    "package:watch": "tsdown --watch",
    "test": "bun test",
    "typecheck": "tsgo --noEmit"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@eslint/compat": "^2.0.0",
    "@github/local-action": "^6.0.2",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "tsdown": "^0.17.1"
  },
  "peerDependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20251207.1",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@types/bun": "latest"
  },
  "packageManager": "bun@1.3.3",
  "engines": {
    "node": ">=24.0.0"
  },
  "volta": {
    "node": "24.11.1"
  }
}
